<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nico.chic_nfc">
    <uses-permission android:name="android.permission.NFC" />
    <uses-sdk android:minSdkVersion="14"/>
    <uses-feature android:name="android.hardware.nfc" android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.nico.chic_nfc.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>

        <activity
            android:name="com.example.nico.chic_nfc.NFC_Activity">
        </activity>

        <activity
            android:name="com.example.nico.chic_nfc.NFC_Read">
        </activity>

        <activity
            android:name="com.example.nico.chic_nfc.Reset_Activity">
        </activity>
    </application>

</manifest>